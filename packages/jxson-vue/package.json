{
  "name": "@rucire/jxson-vue",
  "version": "0.1.0",
  "description": "A powerful JSON editor component for Vue 3 by Rucire",
  "bugs": "https://github.com/rucire/jxSon/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rucire/jxSon.git",
    "directory": "packages/jxson-vue"
  },
  "type": "module",
  "main": "./dist/jxson.umd.js",
  "module": "./dist/jxson.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/jxson.es.js",
      "require": "./dist/jxson.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "lucide-vue-next": "^0.468.0",
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.4.0",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.1.12"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "json",
    "config",
    "editor",
    "ui",
    "rucire"
  ],
  "author": "Rucire",
  "license": "MIT"
}
